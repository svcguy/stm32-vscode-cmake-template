{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "STLINK Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stlink",
            "stlinkPath": "${TEMPLATE_STLINK_PATH}",
            "stm32cubeprogrammer": "${TEMPLATE_CUBE_PROG}",
            "gdbPath": "${TEMPLATE_GDB_PATH}",
            "device": "${TEMPLATE_DEVICE}",
            "svdFile": "${workspaceFolder}\\stm32-svd\\${TEMPLATE_SVD}",
            "runToMain": true
        }
    ]
}